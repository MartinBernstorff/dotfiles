# Focusing windows
ctrl + cmd - h : yabai -m window --focus prev
ctrl + cmd - l : yabai -m window --focus next

# Fullscreen
ctrl + cmd + cmd - f : yabai -m window --toggle zoom-fullscreen

# Moving windows
ctrl + cmd + cmd - w : yabai -m window --swap north
ctrl + cmd + cmd - s : yabai -m window --swap south
ctrl + cmd + cmd - a : yabai -m window --swap west
ctrl + cmd + cmd - d : yabai -m window --swap east

# move window to space #
shift + alt - 1 : yabai -m window --space 1;
shift + alt - 2 : yabai -m window --space 2;
shift + alt - 3 : yabai -m window --space 3;
shift + alt - 4 : yabai -m window --space 4;
shift + alt - 5 : yabai -m window --space 5;
shift + alt - 6 : yabai -m window --space 6;
shift + alt - 7 : yabai -m window --space 7;

# Resize windows
ctrl + cmd - i: yabai -m window --resize right:-335:0 2> /dev/null || yabai -m window --resize left:-335:0 2> /dev/null 
ctrl + cmd - o: yabai -m window --resize right:335:0 2> /dev/null || yabai -m window --resize left:335:0 2> /dev/null 